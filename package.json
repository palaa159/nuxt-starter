    "android 4.4"
  ],
  "scripts": {
    "dev": "yarn; cross-env HOST=0.0.0.0 NODE_ENV=development nodemon server/index.js --watch server",
    "build": "yarn; nuxt build",
    "deploy": "rm -rf nuxt; rm -rf functions/nuxt; nuxt build; cp -R nuxt functions/; cp -R static/* nuxt/dist/; yarn deploy-firebase",
    "start": "NODE_ENV=production node server/index.js",
    "deploy-firebase": "cd functions; rm -rf nuxt/dist/client; yarn; firebase deploy --only functions,hosting"
  },
